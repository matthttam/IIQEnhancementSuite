{
  "manifest_version": 3,
  "name": "IIQ RapidScan Sounds",
  "short_name": "IIQRapidScanSounds",
  "description": "Plays sounds when scanning an assets during rapid scanning.",
  "icons": {},
  "version": "1.0.1",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.incidentiq.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "minimum_chrome_version": "109",
  "web_accessible_resources": [
    {
      "resources": [
        "valid.mp3",
        "boop.wav"
      ],
      "matches": [
        "https://*.incidentiq.com/*"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "webRequest"
  ],
  "host_permissions": [
    "https://*.incidentiq.com/*"
  ]
}